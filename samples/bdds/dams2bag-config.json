{
  "bag":
  {
    "bag_path": "samples/bdds/dams2bag-sample-output/ppmi-sample-1",
    "bag_archiver":"zip",
    "bag_metadata":
    {
      "Source-Organization": "USC Information Sciences Institute, Informatics Systems Research Division",
      "Contact-Name": "Mike D'Arcy",
      "External-Description": "A bag containing PPMI patient summary data and links to images (via fetch.txt) matching the following properties: enrolled, parkinson's, mci, fncdtcog",
      "Internal-Sender-Identifier": "USC-ISI-IRSD"
    }
  },
  "catalog":
  {
    "host": "https://misd-vm-12.isi.edu",
    "path": "/ermrest/catalog/1",
    "username": "",
    "password": "",
    "queries":
    [
      {
        "query_path": "/entity/A:=PPMI:PATIENT/ENROLL_STATUS=Enrolled/APPRDX=Parkinson%27s%20disease/COGSTATE=Mild%20Cognitive%20Impairment%20%28PD-MCI%29/FNCDTCOG=Yes/$A@sort(PATNO)",
        "schema_path": "/schema/PPMI/table/PATIENT",
        "output_name": "PPMI/PATIENT",
        "output_format": "csv"
      },
      {
        "query_path": "/attribute/A:=PPMI:PATIENT/ENROLL_STATUS=Enrolled/APPRDX=Parkinson%27s%20disease/COGSTATE=Mild%20Cognitive%20Impairment%20%28PD-MCI%29/FNCDTCOG=Yes/$A/S:=PATIENT_IMAGES/F:=IMAGE_FILES/URL:=F:uri,LENGTH:=F:bytes,FILENAME:=F:filepath",
        "output_name": "PPMI/IMAGES",
        "output_format": "fetch"
      },
      {
        "query_path": "/attribute/A:=PPMI:PATIENT/ENROLL_STATUS=Enrolled/APPRDX=Parkinson%27s%20disease/COGSTATE=Mild%20Cognitive%20Impairment%20%28PD-MCI%29/FNCDTCOG=Yes/$A/F:=DATA_FILES/URL:=F:uri,LENGTH:=F:bytes,FILENAME:=F:filepath",
        "output_name": "PPMI/BAM",
        "output_format": "globusfetch"
      }
    ]
  }
}
